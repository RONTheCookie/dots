#!/bin/sh
cp ~/.local/bin/marky.part.html /tmp/marky.html
marked $1 >> /tmp/marky.html
echo "</body> </html>" >> /tmp/marky.html
echo -n https://i.ronthecookie.me/$(curl -F file=@/tmp/marky.html -X POST "https://i.ronthecookie.me/upload/$(cat ~/.local/etc/screenshotToken)" | cut -d \" -f 8) | xclip -selection c
notify-send "marky" "Uploaded document"